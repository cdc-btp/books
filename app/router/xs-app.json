{
 "routes": [
  {
   "source": "^/app/(.*)$",
   "target": "$1",
   "localDir": ".",
   "cacheControl": "no-cache, no-store, must-revalidate"
  },
  {
   "source": "^/appconfig/",
   "localDir": ".",
   "cacheControl": "no-cache, no-store, must-revalidate"
  },{
   "source": "^/user-api(.*)",
   "target": "$1",
   "service": "sap-approuter-userapi"
  }, {
   "source": "^/user-api/currentUser$",
   "target": "/currentUser",
   "service": "sap-approuter-userapi"
  },
  {
   "source": "^/admin/(.*)$",
   "target": "/odata/v4/admin/$1",
   "destination": "srv-api",
   "authenticationType": "ias",
   "csrfProtection": false
  },
  {
   "source": "^/catalog/(.*)$",
   "target": "/odata/v4/catalog/$1",
   "destination": "srv-api",
   "authenticationType": "ias",
   "csrfProtection": false
  },
  {
   "source": "^/api/(.*)$",
   "target": "$1",
   "destination": "srv-api",
   "authenticationType": "ias",
   "csrfProtection": false
  }
 ]
}
